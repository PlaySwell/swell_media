
#import_contacts.modal.fade
	.modal-dialog
		.modal-content
			.modal-header
				%button.close{ type: :button, 'data-dismiss' => 'modal', 'aria-hidden' => 'true' }
					&times;
				%h4.modal-title
					Import Contacts
			= form_tag swell_media.import_contact_admin_index_path, multipart: true, class: 'form-inline' do
				.modal-body
					= label_tag :file
					= file_field_tag :file, class: 'form-control'

				.modal-footer
					%button.btn.btn-default{ type: :button, 'data-dismiss' => 'modal' }
						Close
					= submit_tag 'Upload', class: 'btn btn-primary'
